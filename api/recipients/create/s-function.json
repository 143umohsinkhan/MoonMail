{
  "name": "createRecipients",
  "runtime": "babel",
  "description": "Serverless Lambda function for project: MoonMail-v2",
  "customName": "${project}-recipients-create",
  "customRole": false,
  "handler": "recipients/create/handler.default",
  "timeout": 6,
  "memorySize": 128,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "/recipients",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "recipient": "$input.json('$.recipient')"
        }
      },
      "responses": "$${responses}"
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
